    "gh-pages": "^4.0.0",
    "html-webpack-plugin": "^5.5.3",
    "mkdirp": "^1.0.4",
    "prettier": "^2.8.8",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
